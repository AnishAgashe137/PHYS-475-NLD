import numpy as np
from scipy.integrate import odeint
import matplotlib.pyplot as plt

# function that returns dy/dt
def model(x,t):
    dxdt = np.sin(x)
    return dxdt

# initial condition
x0 = np.pi/4.

# time points
t = np.linspace(0,5)

# solve ODE
x = odeint(model,x0,t)

# plot results
plt.plot(t,x)
plt.xlabel('time')
plt.ylabel('x(t)')
plt.show()
